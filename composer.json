{
    "name": "soap/thailand-provinces",
    "description": "Thailand provinces database as Laravel package",
    "type": "laravel-package",
    "keyword" : ["laravel", "thailand", "provinces", "database"],
    "require": {
        "illuminate/support": "5.x"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "laracasts/testdummy": "~2.0",
        "laravel/laravel": "5.2.x"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Prasit Gebsaap",
            "email": "prasit.gebsaap@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Soap\\ThProvinces\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soap\\ThProvinces\\": "packages/soap/thailand-provinces/src"
        },
        "classmap": [
            "tests/DbTestCase.php",
            "tests/ProvincesTest.php"
        ]
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
